<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.fklast.mapper.GapMapper">

    <resultMap id="BaseResultMap" type="com.example.fklast.domain.Gap">
        <id property="gid" column="gid" jdbcType="VARCHAR"/>
        <result property="vid" column="vid" jdbcType="VARCHAR"/>
        <result property="gapContent" column="gap_content" jdbcType="VARCHAR"/>
        <result property="gapLabel" column="gap_label" jdbcType="VARCHAR"/>
        <result property="gapAnswer" column="gap_answer" jdbcType="VARCHAR"/>
        <result property="gapDelete" column="gap_delete" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        gid,vid,gap_content,
        gap_label,gap_answer,gap_delete,
        create_time,update_time
    </sql>
</mapper>
