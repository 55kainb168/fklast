        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.fklast.mapper.JudgeMapper">

    <resultMap id="BaseResultMap" type="com.example.fklast.domain.Judge">
